{
  "name": "@lordabbett/captivate-common-lib",
  "version": "1.0.0",
  "main": "lib/index.js",
  "private": true,
  "files": [
    "/lib"
  ],
  "pre-commit": "precommit",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "description": "Lord Abbett website v3 common components",
  "scripts": {
    "install:io": "yarn install --ignore-optional --network-timeout 60000",
    "clean": "rm -rf lib && rm -rf dist && rm -rf",
    "run:workshop": "start-storybook -p 5000",
    "prebuild:workshop": "yarn run test:generate-output",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "build:workshop": "yarn prebuild:workshop && build-storybook -c .storybook -o dist/workshop",
    "test": "yarn run jest --coverage --logHeapUsage --maxWorkers=2 --config jest.config.js",
    "run:storefront": "styleguidist server",
    "build:storefront": "styleguidist build",
    "build:lib": "./node_modules/.bin/babel -d ./lib ./src",
    "build": "react-scripts build",
    "start": "react-scripts start",
    "analyze": "source-map-explorer build/static/js/main.*",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "precommit": "lint-staged",
    "dev": "webpack -p --progress && clientlib --verbose",
    "prod": "webpack -d --config ./webpack.prod.js"
  },
  "author": "lord abbett",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "~7.7.0",
    "@babel/core": "~7.7.2",
    "@babel/plugin-proposal-class-properties": "~7.7.0",
    "@babel/plugin-proposal-function-bind": "~7.2.0",
    "@babel/plugin-syntax-class-properties": "~7.2.0",
    "@babel/plugin-transform-modules-commonjs": "~7.7.0",
    "@babel/polyfill": "~7.7.0",
    "@babel/preset-env": "~7.7.1",
    "@babel/preset-react": "~7.7.0",
    "@brightcove/react-player-loader": "1.3.0",
    "@material-ui/core": "~4.6.1",
    "@material-ui/icons": "~4.5.1",
    "@material-ui/styles": "~4.6.0",
    "@storybook/addon-a11y": "~5.2.6",
    "@storybook/addon-actions": "~5.2.6",
    "@storybook/addon-backgrounds": "~5.2.6",
    "@storybook/addon-jest": "~5.2.6",
    "@storybook/addon-knobs": "~5.2.6",
    "@storybook/addon-storysource": "~5.2.6",
    "@storybook/addon-viewport": "~5.2.6",
    "@storybook/react": "~5.2.6",
    "@storybook/theming": "~5.2.6",
    "@testing-library/jest-dom": "~4.2.4",
    "@testing-library/react": "~9.3.2",
    "@types/jest": "~24.0.23",
    "add": "~2.0.6",
    "axios": "~0.19.0",
    "babel-eslint": "~10.0.3",
    "babel-jest": "~24.9.0",
    "babel-loader": "~8.0.6",
    "babel-polyfill": "~6.26.0",
    "copy-modules-webpack-plugin": "~2.0.4",
    "css-loader": "^3.2.1",
    "dotenv-webpack": "~1.7.0",
    "enzyme": "~3.10.0",
    "enzyme-adapter-react-16": "~1.15.1",
    "enzyme-to-json": "~3.4.3",
    "eslint-config-google": "~0.14.0",
    "eslint-plugin-jsx-a11y": "~6.2.3",
    "eslint-plugin-react": "~7.16.0",
    "file-loader": "~4.2.0",
    "flag-icon-css": "3.4.5",
    "jest": "~24.9.0",
    "lint-staged": "~9.4.3",
    "prop-types": "~15.7.2",
    "react-a11y": "~1.1.0",
    "react-axe": "~3.3.0",
    "react-ga": "~2.7.0",
    "react-perfect-scrollbar": "~1.5.3",
    "react-redux": "~7.1.3",
    "react-router": "~5.1.2",
    "react-select": "3.0.8",
    "react-share": "~3.0.1",
    "react-styleguidist": "~10.2.1",
    "react-test-renderer": "~16.12.0",
    "react-transition-group": "~4.3.0",
    "styled-components": "~4.4.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "yarn": "~1.19.1"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
